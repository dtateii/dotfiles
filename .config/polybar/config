[module/bluetooth]
type = custom/script
exec = bluetoothctl -- show | grep -q "Powered: yes" && echo "ïŠ”  on" || echo "%{F#899}ïŠ”  off%{F-}"
interval = 5
click-left = bluetoothctl -- power on
click-right = bluetoothctl -- power off

[module/vpn-nord]
type = custom/script
exec = nordvpn status | grep -q "Current server: " && echo "%{F#9FE} ï„²  Secure%{F-} " || echo "%{F#f66} ï„²  Not Secure %{F-}"
interval = 5
click-left = nordvpn connect
click-right = nordvpn disconnect

[module/vpn-kerio]
